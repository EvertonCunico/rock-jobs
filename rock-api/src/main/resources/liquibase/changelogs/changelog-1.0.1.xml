<?xml version="1.1" encoding="UTF-8" standalone="no"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                   xsi:schemaLocation=" http://www.liquibase.org/xml/ns/dbchangelog                                        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">    <changeSet id="add-complemento-endereco-usuario" author="everton.cunico">        <addColumn tableName="usuario">            <column name="endereco_complemento" type="varchar(40)"></column>        </addColumn>    </changeSet>    <changeSet id="add-campos-vaga" author="everton.cunico">        <addColumn tableName="vaga">            <column name="trabalha_domingo" type="boolean"></column>            <column name="trabalha_segunda" type="boolean"></column>            <column name="trabalha_terca" type="boolean"></column>            <column name="trabalha_quarta" type="boolean"></column>            <column name="trabalha_quinta" type="boolean"></column>            <column name="trabalha_sexta" type="boolean"></column>            <column name="trabalha_sabado" type="boolean"></column>        </addColumn>    </changeSet></databaseChangeLog>