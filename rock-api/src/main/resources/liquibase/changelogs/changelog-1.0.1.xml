<?xml version="1.1" encoding="UTF-8" standalone="no"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                   xsi:schemaLocation=" http://www.liquibase.org/xml/ns/dbchangelog                                        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">    <changeSet id="add-complemento-endereco-usuario" author="everton.cunico">        <addColumn tableName="usuario">            <column name="endereco_complemento" type="varchar(40)"></column>        </addColumn>    </changeSet>    <changeSet id="add-campos-vaga" author="everton.cunico">        <addColumn tableName="vaga">            <column name="trabalha_domingo" type="boolean"></column>            <column name="trabalha_segunda" type="boolean"></column>            <column name="trabalha_terca" type="boolean"></column>            <column name="trabalha_quarta" type="boolean"></column>            <column name="trabalha_quinta" type="boolean"></column>            <column name="trabalha_sexta" type="boolean"></column>            <column name="trabalha_sabado" type="boolean"></column>        </addColumn>    </changeSet>    <changeSet id="correcao-vaga-situacao" author="everton.cunico">        <sql>alter table vaga alter column situacao type varchar(20);</sql>    </changeSet>    <changeSet id="create-table-email" author="everton.cunico">        <createTable tableName="email">            <column name="id" type="BIGINT" autoIncrement="true" >                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="nome_destinatario" type="VARCHAR(100)">                <constraints nullable="false"/>            </column>            <column name="endereco_email" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="data" type="TIMESTAMP">                <constraints nullable="false"/>            </column>            <column name="estado" type="VARCHAR(20)">                <constraints nullable="false"/>            </column>            <column name="modelo" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="USUARIO_ID" type="BIGINT"/>        </createTable>        <addForeignKeyConstraint baseTableName="EMAIL" baseColumnNames="USUARIO_ID" constraintName="fk_usuario"                                 referencedTableName="USUARIO"                                 referencedColumnNames="ID"/>    </changeSet>        <changeSet id="altera-ramo-atuacao-empresa" author="everton.cunico">        <sql>alter table empresa alter ramo_atuacao type varchar(100);</sql>    </changeSet>    <changeSet id="complemeto-empresa" author="everton.cunico">        <addColumn tableName="empresa">            <column name="complemento" type="text"></column>        </addColumn>    </changeSet>    <changeSet id="segundo-turno-empresa" author="everton.cunico">        <addColumn tableName="vaga">            <column name="contraturno_segunda_inicio" type="time"/>            <column name="contraturno_segunda_fim" type="time"/>            <column name="contraturno_terca_inicio" type="time"/>            <column name="contraturno_terca_fim" type="time"/>            <column name="contraturno_quarta_inicio" type="time"/>            <column name="contraturno_quarta_fim" type="time"/>            <column name="contraturno_quinta_inicio" type="time"/>            <column name="contraturno_quinta_fim" type="time"/>            <column name="contraturno_sexta_inicio" type="time"/>            <column name="contraturno_sexta_fim" type="time"/>            <column name="contraturno_sabado_inicio" type="time"/>            <column name="contraturno_sabado_fim" type="time"/>            <column name="contraturno_domingo_inicio" type="time"/>            <column name="contraturno_domingo_fim" type="time"/>        </addColumn>    </changeSet></databaseChangeLog>