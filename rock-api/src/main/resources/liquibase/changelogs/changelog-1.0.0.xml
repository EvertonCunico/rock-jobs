<?xml version="1.1" encoding="UTF-8" standalone="no"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd    http://www.liquibase.org/xml/ns/dbchangelog    https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">    <changeSet id="add-tabela-usuario" author="everton.cunico">        <createTable tableName="usuario">            <column name="id" type="BIGINT" autoIncrement="true">                <constraints primaryKey="true"/>            </column>            <column name="nome" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="cpf" type="VARCHAR(14)">                <constraints unique="true"/>            </column>            <column name="rg" type="VARCHAR(20)"/>            <column name="data_nascimento" type="DATE"/>            <column name="email" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="telefone_celular" type="VARCHAR(20)"/>            <column name="login" type="VARCHAR(50)">                <constraints nullable="false" unique="true"/>            </column>            <column name="senha" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="ativo" type="BOOLEAN">                <constraints nullable="false"/>            </column>            <column name="tipo_acesso" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>        </createTable>    </changeSet>    <changeSet id="add-usuario-padrao" author="everton.cunico">        <sql>INSERT INTO public.usuario (nome, email,