<?xml version="1.1" encoding="UTF-8" standalone="no"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                   xsi:schemaLocation=" http://www.liquibase.org/xml/ns/dbchangelog                                        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">    <changeSet id="cidades_estados" author="everton.cunico">        <!-- PaÃ­ses -->        <sqlFile path="liquibase/sql/pais.sql"/>        <!-- Estados -->        <sqlFile path="liquibase/sql/estado.sql"/>        <!-- Cidades -->        <sqlFile path="liquibase/sql/cidade.sql"/>    </changeSet>    <changeSet id="add-tabela-empresa" author="everton.cunico">        <createTable tableName="empresa">            <column name="id" type="bigint" autoIncrement="true">                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="nome_fantasia" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="razao_social" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="foto" type="UUID"/>            <column name="cnpj" type="varchar(14)">                <constraints nullable="false" unique="true"/>            </column>            <column name="inscricao_estadual" type="varchar(20)"/>            <column name="telefone" type="varchar(20)"/>            <column name="email" type="varchar(100)">                <constraints nullable="false"/>            </column>            <column name="endereco" type="varchar(255)"/>            <column name="endereco_numero" type="varchar(20)"/>            <column name="endereco_bairro" type="varchar(100)"/>            <column name="endereco_cep" type="varchar(8)"/>            <column name="cidade_id" type="bigint"/>            <column name="numero_colaboradores" type="varchar(11)"/>            <column name="area_atuacao" type="varchar(20)"/>            <column name="ramo_atuacao" type="varchar(11)"/>            <column name="responsavel_empresa" type="varchar(255)"/>            <column name="telefone_responsavel_empresa" type="varchar(20)"/>            <column name="responsavel_rh" type="varchar(255)"/>            <column name="telefone_responsavel_rh" type="varchar(20)"/>        </createTable>        <addForeignKeyConstraint baseTableName="empresa" baseColumnNames="cidade_id"                                 constraintName="fk_empresa_cidade"                                 referencedTableName="cidade"                                 referencedColumnNames="cidade_id"/>    </changeSet>    <changeSet id="add-tabela-usuario" author="everton.cunico">        <createTable tableName="usuario">            <column name="id" type="BIGINT" autoIncrement="true">                <constraints primaryKey="true"/>            </column>            <column name="nome" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="foto" type="UUID"/>            <column name="sobrenome" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="cpf" type="VARCHAR(14)">                <constraints unique="true"/>            </column>            <column name="data_nascimento" type="DATE"/>            <column name="telefone_celular" type="VARCHAR(20)"/>            <column name="endereco" type="VARCHAR(255)"/>            <column name="email" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="senha" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="ativo" type="BOOLEAN">                <constraints nullable="false"/>            </column>            <column name="tipo_acesso" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="empresa_id" type="bigint"/>            <column name="data_inclusao" type="timestamp"/>        </createTable>        <addForeignKeyConstraint baseTableName="usuario" baseColumnNames="empresa_id"                                 constraintName="fk_usuario_empresa"                                 referencedTableName="empresa"                                 referencedColumnNames="id"/>    </changeSet>    <changeSet id="add-usuario-padrao" author="everton.cunico">        <sql>INSERT INTO public.usuario (nome, sobrenome, email, senha, ativo, tipo_acesso)            VALUES('ADMIN', 'ROCK JOBS', 'admin@rockjobs.com', '$2a$10$0iCN3FJqtzau21ALi4Rc9.6x6WqTBt25xN6Y8Pxk8PbEX1vsQqkeu', true, 'ADMIN_GERAL');</sql>    </changeSet>    <changeSet id="add-tabela-vagas" author="everton.cunico">        <createTable tableName="vaga">            <column name="id" type="bigint" autoIncrement="true">                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="empresa_id" type="bigint">                <constraints nullable="false"/>            </column>            <column name="foto" type="UUID"/>            <column name="nome_da_funcao" type="varchar(120)">                <constraints nullable="false"/>            </column>            <column name="quantidade_vagas" type="integer">                <constraints nullable="false"/>            </column>            <column name="vaga_sigilosa" type="boolean">                <constraints nullable="false"/>            </column>            <column name="data_limite_selecao" type="date">                <constraints nullable="false"/>            </column>            <column name="data_limite_integracao" type="date">                <constraints nullable="false"/>            </column>            <column name="atribuicao_sumaria" type="text"/>            <column name="atividades_tipicas" type="text"/>            <column name="atividades_eventuais" type="text"/>            <column name="nivel_autoridade_responsabilidade" type="text"/>            <column name="habilidades_necessarias" type="text"/>            <column name="requisitos_basicos" type="text"/>            <column name="requisitos_desejaveis" type="text"/>            <column name="escolaridade" type="varchar(30)">                <constraints nullable="false"/>            </column>            <column name="cursos_obrigatorios" type="text"/>            <column name="tipo_contrato" type="varchar(20)">                <constraints nullable="false"/>            </column>            <column name="carga_horaria_semanal" type="text">                <constraints nullable="false"/>            </column>            <column name="remuneracao" type="text"/>            <column name="informa_comissoes_bonus" type="boolean"/>            <column name="comissoes_bonus" type="text"/>            <column name="situacao" type="varchar(10)"/>            <column name="vale_alimentacao" type="boolean"/>            <column name="vale_transporte" type="boolean"/>            <column name="vale_refeicao" type="boolean"/>            <column name="informacoes_rock" type="text"/>            <column name="segunda_feira_inicio" type="time"/>            <column name="segunda_feira_fim" type="time"/>            <column name="terca_feira_inicio" type="time"/>            <column name="terca_feira_fim" type="time"/>            <column name="quarta_feira_inicio" type="time"/>            <column name="quarta_feira_fim" type="time"/>            <column name="quinta_feira_inicio" type="time"/>            <column name="quinta_feira_fim" type="time"/>            <column name="sexta_feira_inicio" type="time"/>            <column name="sexta_feira_fim" type="time"/>            <column name="sabado_inicio" type="time"/>            <column name="sabado_fim" type="time"/>            <column name="domingo_inicio" type="time"/>            <column name="domingo_fim" type="time"/>            <column name="informacao_rock" type="text"/>            <column name="genero" type="varchar(10)"/>            <column name="sequencial_por_empresa" type="bigint"/>            <column name="data_inclusao" type="timestamp"/>        </createTable>    </changeSet></databaseChangeLog>