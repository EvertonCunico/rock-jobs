<?xml version="1.1" encoding="UTF-8" standalone="no"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"                   xsi:schemaLocation=" http://www.liquibase.org/xml/ns/dbchangelog                                        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">    <changeSet id="cidades_estados" author="everton.cunico">        <!-- PaÃ­ses -->        <sqlFile path="liquibase/sql/pais.sql"/>        <!-- Estados -->        <sqlFile path="liquibase/sql/estado.sql"/>        <!-- Cidades -->        <sqlFile path="liquibase/sql/cidade.sql"/>    </changeSet>    <changeSet id="add-tabela-cliente" author="everton.cunico">        <createTable tableName="cliente">            <column name="id" type="bigint" autoIncrement="true">                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="nome_fantasia" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="razao_social" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="cnpj" type="varchar(14)">                <constraints nullable="false"/>            </column>            <column name="inscricao_estadual" type="varchar(20)"/>            <column name="telefone" type="varchar(20)"/>            <column name="email" type="varchar(100)">                <constraints nullable="false"/>            </column>            <column name="endereco" type="varchar(255)"/>            <column name="endereco_numero" type="varchar(20)"/>            <column name="endereco_bairro" type="varchar(100)"/>            <column name="endereco_cep" type="varchar(8)"/>            <column name="cidade_id" type="bigint">                <constraints nullable="false"/>            </column>            <column name="numero_colaboradores" type="varchar(11)"/>            <column name="area_atuacao" type="varchar(20)"/>            <column name="ramo_atuacao" type="varchar(11)"/>            <column name="responsavel_rh" type="varchar(255)"/>            <column name="telefone_responsavel_rh" type="varchar(20)"/>        </createTable>        <addForeignKeyConstraint baseTableName="cliente" baseColumnNames="cidade_id"                                 constraintName="fk_cliente_cidade"                                 referencedTableName="cidade"                                 referencedColumnNames="cidade_id"/>    </changeSet>    <changeSet id="add-tabela-usuario" author="everton.cunico">        <createTable tableName="usuario">            <column name="id" type="BIGINT" autoIncrement="true">                <constraints primaryKey="true"/>            </column>            <column name="nome" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="sobrenome" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="cpf" type="VARCHAR(14)">                <constraints unique="true"/>            </column>            <column name="data_nascimento" type="DATE"/>            <column name="telefone_celular" type="VARCHAR(20)"/>            <column name="endereco" type="VARCHAR(255)"/>            <column name="email" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="senha" type="VARCHAR(100)">                <constraints unique="true"/>            </column>            <column name="ativo" type="BOOLEAN">                <constraints nullable="false"/>            </column>            <column name="tipo_acesso" type="VARCHAR(255)">                <constraints nullable="false"/>            </column>            <column name="cliente_id" type="bigint"/>        </createTable>        <addForeignKeyConstraint baseTableName="usuario" baseColumnNames="cliente_id"                                 constraintName="fk_usuario_cliente"                                 referencedTableName="cliente"                                 referencedColumnNames="id"/>    </changeSet>    <changeSet id="add-usuario-padrao" author="everton.cunico">        <sql>INSERT INTO public.usuario (nome, sobrenome, email, senha, ativo, tipo_acesso)            VALUES('ADMIN', 'ROCK JOBS', 'admin@rockjobs.com', '$2a$10$0iCN3FJqtzau21ALi4Rc9.6x6WqTBt25xN6Y8Pxk8PbEX1vsQqkeu', true, 'ADMIN_GERAL');</sql>    </changeSet></databaseChangeLog>